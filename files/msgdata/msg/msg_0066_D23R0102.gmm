	<row id="msg_0066_D23R0102_00019" index="19">
		<attribute name="window_context_name">used</attribute>
		<language name="English">Buena: Come back after you listen to\nmy show, OK? Catch ya later!</language>
	</row>
	<row id="msg_0066_D23R0102_00020" index="20">
		<attribute name="window_context_name">used</attribute>
		<language name="English">Buena: Everyone ready?\rI want to hear you shout out today’s\npassword for {STRVAR_1 3, 0, 0}!\r</language>
	</row>
	<row id="msg_0066_D23R0102_00021" index="21">
		<attribute name="window_context_name">used</attribute>
